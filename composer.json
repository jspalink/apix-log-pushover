{
  "name": "jspalink/apix-log-pushover",
  "description": "A Pushover logger for APIx Log",
  "type": "library",
  "minimum-stability": "stable",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Jonathan Spalink",
      "email": "jspalink@info.com"
    },
    {
      "name": "Franck Cassedanne",
      "email": "franck@ouarz.net"
    }
  ],
  "require": {
    "php": ">=5.3",
    "apix/log": "dev-1.2.0-beta",
    "sqmk/pushy": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "psr-4": { "Apix\\Log\\Logger\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Apix\\Log\\Logger\\tests\\": "tests/" }
  },
  "provide": {
    "psr/log-implementation": "1.0.0"
  },
  "repositories": [
        {
            "type": "git",
            "url": "https://github.com/frqnck/apix-log"
        }
    ]
}
